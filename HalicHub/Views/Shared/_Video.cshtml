@model Video

<div class="col-lg-4 col-md-6 col-sm-6 mix branding">
    <div class="portfolio__item">
        <div class="portfolio__item__video set-bg" data-setbg="/img/portfolio-3.jpg">
            <button type="button" class="video-btn play-btn" data-toggle="modal" data-src="https://www.youtube.com/embed/IP7uGKgJL8U" data-target="#myModal">
                <i class="fa fa-play"></i>
            </button>
        </div>
        <div class="portfolio__item__text">
            <h4>@Model.Tittle</h4>
            <span>@Model.Date</span>
        </div>
    </div>
</div>